{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "dev": {
      "outputs": [],
      "dependsOn": [],
      "persistent": true,
      "inputs": ["apps/**"]
    },
    "preview": {
      "outputs": [],
      "dependsOn": [],
      "persistent": true,
      "inputs": ["apps/**"]
    },
    "prebuild": {
      "outputs": [],
      "dependsOn": []
    }
  }
}
